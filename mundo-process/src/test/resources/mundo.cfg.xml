    <activity id="BUYER_DIRECTOR_BUYER_UPDATE" name="经理驳回采购修改"/>
    <activity id="MASTER_BUYER_UPDATE" name="主档驳回采购修改"/>
    <activity id="MASTER_APPROVE" name="主档审批"/>
    <exclusiveGateway id="BUYER-APPROVE-GATEWAY" name="采购审批网关"/>
    <exclusiveGateway id="BUYER-MANAGEMENT-GATEWAY" name="采购经历审批网关"/>
    <exclusiveGateway id="BUYER-DIRECTOR-GATEWAY" name="采购总监审批网关"/>
    <exclusiveGateway id="MASTER-GATEWAY" name="主档审批网关"/>
    <parallelGateway id="PARALLEL-GATEWAY" name="并行审批网关"/>
    <link id="START->SUP_CREATE" name="START->SUP_CREATE" sourceId="START" targetId="SUP_CREATE"/>
    <link id="SUP_CREATE->BUYER_APPROVE" name="SUP_CREATE->BUYER_APPROVE" sourceId="SUP_CREATE" targetId="BUYER_APPROVE"/>
    <link id="BUYER_APPROVE->BUYER-APPROVE-GATEWAY" name="BUYER_APPROVE->BUYER-APPROVE-GATEWAY" sourceId="BUYER_APPROVE" targetId="BUYER-APPROVE-GATEWAY"/>
    <link id="BUYER-APPROVE-GATEWAY->SUP_UPDATE" name="BUYER-APPROVE-GATEWAY->SUP_UPDATE" sourceId="BUYER-APPROVE-GATEWAY" targetId="SUP_UPDATE">
      <conditionExpression>#buyerApprove == false</conditionExpression>
    </link>
    <link id="SUP_UPDATE->BUYER_APPROVE" name="SUP_UPDATE->BUYER_APPROVE" sourceId="SUP_UPDATE" targetId="BUYER_APPROVE" />
    <link id="BUYER-APPROVE->PARALLEL-GATEWAY" name="BUYER-APPROVE->PARALLEL-GATEWAY" sourceId="BUYER-APPROVE-GATEWAY" targetId="PARALLEL-GATEWAY">
      <conditionExpression>#buyerApprove == true</conditionExpression>
    </link>
    <link id="PARALLEL-GATEWAY->BUYER_MANAGEMENT_APPROVE" name="PARALLEL-GATEWAY->BUYER_MANAGEMENT_APPROVE" sourceId="PARALLEL-GATEWAY" targetId="BUYER_MANAGEMENT_APPROVE"/>
    <link id="PARALLEL-GATEWAY->BUYER_DIRECTOR_APPROVE" name="PARALLEL-GATEWAY->BUYER_DIRECTOR_APPROVE" sourceId="PARALLEL-GATEWAY" targetId="BUYER_DIRECTOR_APPROVE"/>
    <link id="BUYER_MANAGEMENT_APPROVE->BUYER_MANAGEMENT_BUYER_UPDATE" name="BUYER_MANAGEMENT_APPROVE->BUYER_MANAGEMENT_BUYER_UPDATE" sourceId="BUYER_MANAGEMENT_APPROVE" targetId="BUYER_MANAGEMENT_BUYER_UPDATE">
      <conditionExpression>#buyerManagementApprove == false</conditionExpression>
    </link>
    <link id="BUYER_MANAGEMENT_BUYER_UPDATE->BUYER_MANAGEMENT_APPROVE" name="BUYER_MANAGEMENT_BUYER_UPDATE->BUYER_MANAGEMENT_APPROVE" sourceId="BUYER_MANAGEMENT_BUYER_UPDATE" targetId="BUYER_MANAGEMENT_APPROVE"/>
    <link id="BUYER_MANAGEMENT_APPROVE->MASTER_APPROVE" name="BUYER_MANAGEMENT_APPROVE->MASTER_APPROVE" sourceId="BUYER_MANAGEMENT_APPROVE" targetId="MASTER_APPROVE">
      <conditionExpression>#buyerManagementApprove == true</conditionExpression>
    </link>
    <link id="BUYER_DIRECTOR_APPROVE->BUYER_DIRECTOR_BUYER_UPDATE" name="BUYER_DIRECTOR_APPROVE->BUYER_UPDATE" sourceId="BUYER_DIRECTOR_APPROVE" targetId="BUYER_DIRECTOR_BUYER_UPDATE">
