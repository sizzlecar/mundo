  <process id="parallel-process-1" name="并行流程" version="0">
    <start id="START" name="开始"/>
    <activity id="SUP_CREATE" name="供应商新建"/>
    <activity id="BUYER_APPROVE" name="采购审批"/>
    <activity id="SUP_UPDATE" name="供应商修改"/>
